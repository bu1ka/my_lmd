{
    "name": "main build",
    "root": "../",
    "output": "./i/js/dist/main.lmd.js",
    "modules": {
        "lodash": {
            "path": "./i/js/vendors/lodash/dist/lodash.js",
            "exports": "_"
        },
        "underscore": {
            "path": "./i/js/vendors/lodash/dist/lodash.underscore.js",
            "exports": "_"
        },
        "jquery": {
            "path": "./i/js/vendors/jquery/jquery.js",
            "exports": "$"
        },
        "Backbone" : {
            "path" : "./i/js/vendors/backbone/backbone.js",
            "require": {
                "_": "lodash",
                "$": "jquery"
            },

            "exports": "Backbone"
        },

        "RedditModel": "./i/app/js/Models/RedditModel.js",
        "RedditView" : "./i/app/js/Views/RedditView.js",
        "RedditTemplate": "./i/app/templates/reddit.html",
        "RedditsView": "./i/app/js/Views/RedditsView.js",
        "RedditsCollection": "./i/app/js/Collections/RedditsCollection.js",
        "main": "./i/app/js/app.js"
    },
    "main": "main",

    "log": true,
    "warn": true

}
